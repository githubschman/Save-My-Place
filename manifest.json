{
    "name": "Save My Place",
    "description": "Saves your place on a blog post, article, or anything else you're reading.",
    "background": {
        "scripts":  ["js/jquery-3.2.1.js","js/background.js"],
        "css":["style.css"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["style.css"],
            "js": ["js/jquery-3.2.1.js", "js/content.js"]
        }
    ],
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Save My Place",
        "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage",
      "tabs",
      "webNavigation"
    ]
}



